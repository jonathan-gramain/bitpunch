let RadioInt = integer { signed: false; endian: 'big'; };
let u8  = byte[1]: RadioInt;
let u16 = byte[2]: RadioInt;
let u32 = byte[4]: RadioInt;
let u64 = byte[8]: RadioInt;

let RadioCode = struct {
       foo: byte[6];
       item_span: struct { value: u16; };
       codename: byte[]: string { boundary: '\0'; };
       key codename;
       span item_span.value;
};

file {
     n_codes: u32;
     codes: RadioCode[n_codes];
}
